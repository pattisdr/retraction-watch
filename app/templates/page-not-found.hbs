{{title 'Page not found'}}

<div class="preprint-404">
    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <h1>{{t "page-not-found.heading"}}</h1>
                <p>
                    <span>{{t "page-not-found.paragraph.line1"}}</span>
                    <br>
                    <span>{{t "page-not-found.paragraph.line2"}}</span>
                    <a href="mailto:support@osf.io"
                       onbeforeclick={{action "click" "link" "Page not found - Support"}}>
                        {{"support@osf.io"}}
                    </a>
                </p>
                    {{#link-to "index" class="btn btn-primary m-t-md" invokeAction=(action "click" "link" "Page not found - Go to Index")}}
                        {{t "page-not-found.go_to" service=(t "global.retraction_watch")}}
                    {{/link-to}}
            </div>
        </div>
    </div>
</div>
